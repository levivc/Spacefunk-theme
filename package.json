{
  "name": "spacefunk-theme",
  "displayName": "Spacefunk Theme",
  "description": "Groovy Baby!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Spacefunk",
        "uiTheme": "vs-dark",
        "path": "./themes/Spacefunk-color-theme.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "spacefunk.colon.injection",
        "path": "./syntaxes/colon.injection.json",
        "injectTo": [
          "source.lua",
          "source.js",
          "source.ts",
          "source.python"
        ]
      }
    ]
  }
}
